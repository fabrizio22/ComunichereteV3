icherete_blog_layout_options(){\n	$layouts = array();\n	foreach( glob(get_template_directory().'/loops/loop-*.php') as $template ) {\n		$headers = get_file_data( $template, array(\n			'loop_name' => 'Loop Name',\n		) );\n\n		preg_match('/loop\\-(.*?)\\.php/', basename($template), $matches);\n		if(!empty($matches[1])) {\n			$layouts[$matches[1]] = $headers['loop_name'];\n		}\n	}\n\n	static $exclude = array(\n		'carousel', 'slider'\n	);\n\n	foreach($exclude as $e) unset($layouts[$e]);\n	return $layouts;\n}\n\nfunction Comunicherete_feature_suggestion_url($url){\n	return 'http://sorig.in/Comunicherete-suggestions';\n}\nadd_filter('siteorigin_settings_suggest_features_url', 'Comunicherete_feature_suggestion_url');",
