?>\" style=\"<?php foreach($layer_style as $a => $v) echo $a.': '.$v ?>\">\n						<?php echo $content ?>\n					</div>\n				</div>\n			</div>\n		</div>\n	</div>\n	<?php\n\n	return ob_get_clean();\n}",
