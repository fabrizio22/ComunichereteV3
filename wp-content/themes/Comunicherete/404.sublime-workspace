set($sections[$section])) : ?><optgroup label=\"<?php echo esc_attr($sections[$section]) ?>\"><?php endif; ?>\n				<?php foreach($s_icons as $icon) : ?>\n					<option value=\"<?php echo esc_attr($icon) ?>\" <?php selected($current, $icon) ?>><?php echo esc_html(Comunicherete_icon_get_name($icon)) ?></option>\n				<?php endforeach; ?>\n			</optgroup>\n		<?php endforeach; ?>\n	</select>\n	<?php\n	wp_nonce_field('save_post_icon', '_Comunicherete_menuicon_nonce');\n}\n\n/**\n * @param $icon\n * @return string\n */\nfunction Comunicherete_icon_get_name($icon){\n	$name = str_replace('icon-', '', $icon);\n	$name = str_replace('-', ' ', $name);\n	$name = ucwords($name);\n	return $name;\n}\n\n/**\n * Save tge post icon setting\n *\n * @param $post_id\n */\nfunction Comunicherete_menu_icon_save($post_id){\n	if(empty($_POST['_Comunicherete_menuicon_nonce']) || !wp_verify_nonce($_POST['_Comunicherete_menuicon_nonce'], 'save_post_icon')) return;\n	if(!current_user_can('edit_post', $post_id));\n	update_post_meta($post_id, 'Comunicherete_menu_icon', $_POST['Comunicherete_menu_icon']);\n}\nadd_action('save_post', 'Comunicherete_menu_icon_save');",
