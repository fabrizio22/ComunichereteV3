			"contents": "<?php\n/**\n * Custom template tags for this theme.\n *\n * @package Comunicherete\n * @since Comunicherete 1.0\n * @license GPL 2.0\n */\n\nif ( ! function_exists( 'Comunicherete_content_nav' ) ) :\n/**\n * Display navigation to next/previous pages when applicable\n *\n * @since Comunicherete 1.0\n */\nfunction Comunicherete_content_nav( $nav_id ) {\n	global $wp_query, $post;\n\n	// Don't print empty markup on single pages if there's nowhere to navigate.\n	if ( is_single() ) {\n		$previous = ( is_attachment() ) ? get_post( $post->post_parent ) : get_adjacent_post( false, '', true );\n		$next = get_adjacent_post( false, '', false );\n\n		if ( ! $next && ! $previous )\n			return;\n	}\n\n	// Don't print empty markup in archives if there's only one page.\n	if ( $wp_query->max_num_pages < 2 && ( is_home() || is_archive() || is_search() ) )\n		return;\n\n	$nav_class = 'site-navigation paging-navigation';\n	if ( is_single() )\n		$nav_class = 'site-navigation post-navigation';\n\n	?>\n	<nav role=\"navigation\" id=\"<?php echo esc_attr( $nav_id ); ?>\" class=\"<?php echo $nav_class; ?>\">\n		<h1 class=\"assistive-text\"><?php _e( 'Post navigation', 'Comunicherete' ); ?></h1>\n\n	<?php if ( is_single() ) : // navigation links for single posts ?>\n\n		<div class=\"single-nav-wrapper\">\n			<?php previous_post_link( '<div class=\"nav-previous\">%link</div>', '<span class=\"meta-nav\">' . _x( '&larr;', 'Previous post link', 'Comunicherete' ) . '</span> %title' ); ?>\n			<?php next_post_link( '<div class=\"nav-next\">%link</div>', '%title <span class=\"meta-nav\">' . _x( '&rarr;', 'Next post link', 'Comunicherete' ) . '</span>' ); ?>\n		</div>\n\n	<?php elseif ( $wp_query->max_num_pages > 1 && ( is_home() || is_archive() || is_search() ) ) : // navigation links for home, archive, and search pages ?>\n\n		<?php Comunicherete_pagination() ?>\n\n	<?php endif; ?>\n\n	</nav><!-- #<?php echo esc_html( $nav_id ); ?> -->\n	<?php\n}\nendif; // Comunicherete_content_nav\n\nif ( ! function_exists( 'Comunicherete_comment' ) ) :\n/**\n * Template for comments and pingbacks.\n *\n * Used as a callback by wp_list_comments() for displaying the comments.\n *\n * @since Comunicherete 1.0\n */\nfunction Comunicherete_comment( $comment, $args, $depth ) {\n	$GLOBALS['comment'] = $comment;\n	switch ( $comment->comment_type ) :\n		case 'pingback' :\n		case 'trackback' :\n			?>\n			<li class=\"post pingback\">\n				<p><?php _e( 'Pingback:', 'Comunicherete' ); ?> <?php comment_author_link(); ?><?php edit_comment_link( __( '(Edit)', 'Comunicherete' ), ' ' ); ?></p>\n			<?php\n			break;\n		default :\n			?>\n			<li <?php comment_class(); ?> id=\"li-comment-<?php comment_ID(); ?>\">\n				<article id=\"comment-<?php comment_ID(); ?>\" class=\"comment\">\n					<footer>\n						<?php echo get_avatar( $comment, 50 ); ?>\n						<div class=\"comment-author\">\n							<cite class=\"fn\"><?php comment_author_link() ?></cite>\n						</div><!-- .comment-author -->\n\n\n						<div class=\"comment-meta commentmetadata\">\n							<a href=\"<?php echo esc_url( get_comment_link( $comment->comment_ID ) ); ?>\"><time datetime=\"<?php comment_time( 'c' ); ?>\"><?php\n								/* translators: 1: date, 2: time */\n								printf( __( '%1$s at %2$s', 'Comunicherete' ), get_comment_date(), get_comment_time() );\n								?></time></a>\n\n							<span class=\"support\">\n								<?php comment_reply_link( array_merge( $args, array( 'depth' => $depth, 'max_depth' => $args['max_depth'] ) ) ); ?>\n								<?php edit_comment_link( __( 'Edit', 'Comunicherete' ), ' ' ); ?>\n							</span>\n						</div><!-- .comment-meta .commentmetadata -->\n\n						<?php if ( $comment->comment_approved == '0' ) : ?>\n							<em class=\"awaiting-moderation\"><?php _e( 'Your comment is awaiting moderation.', 'Comunicherete' ); ?></em>\n						<?php endif; ?>\n					</footer>\n\n					<div class=\"comment-content entry-content\"><?php comment_text(); ?></div>\n				</article><!-- #comment-## -->\n		\n			<?php\n			break;\n	endswitch;\n}\nendif; // ends check for Comunicherete_comment()\n\nif ( ! function_exists( 'Comunicherete_posted_on' ) ) :\n/**\n * Prints HTML with meta information for the current post-date/time and author.\n *\n * @since Comunicherete 1.0\n */\nfunction Comunicherete_posted_on() {\n	$posted_on_parts = array(\n		'on' => __('Posted on <a href=\"%1$s\" title=\"%2$s\" rel=\"bookmark\"><time class=\"entry-date\" datetime=\"%3$s\">%4$s</time></a>', 'Comunicherete'),\n		'by' => __( '<span class=\"byline\"> by <span class=\"author vcard\"><a class=\"url fn n\" href=\"%5$s\" title=\"%6$s\" rel=\"author\">%7$s</a></span></span>', 'Comunicherete' ),\n	);\n	$posted_on_parts = apply_filters('Comunicherete_post_on_parts', $posted_on_parts);\n\n\n	$posted_on = sprintf( implode(' ', $posted_on_parts),\n		esc_url( get_permalink() ),\n		esc_attr( get_the_time() ),\n		esc_attr( get_the_date( 'c' ) ),\n		apply_filters('Comunicherete_post_on_date', esc_html( get_the_date() )),\n		esc_url( get_author_posts_url( get_the_author_meta( 'ID' ) ) ),\n		esc_attr( sprintf( __( 'View all posts by %s', 'Comunicherete' ), get_the_author() ) ),\n		get_the_author()\n	);\n	echo apply_filters('Comunicherete_posted_on', $posted_on);\n}\nendif;\n\nif(!function_exists('Comunicherete_display_logo')):\n/**\n * Display the logo \n */\nfunction Comunicherete_display_logo(){\n	$logo = siteorigin_setting( 'logo_image' );\n	$logo = apply_filters('Comunicherete_logo_image_id', $logo);\n\n	if( empty($logo) ) {\n		// Just display the site title\n		$logo_html = '<h1 class=\"site-title\">'.get_bloginfo( 'name' ).'</h1>';\n		$logo_html = apply_filters('Comunicherete_logo_text', $logo_html);\n	}\n	else {\n		// load the logo image\n		if(is_array($logo)) {\n			list ($src, $height, $width) = $logo;\n		}\n		else {\n			$image = wp_get_attachment_image_src($logo, 'full');\n			$src = $image[0];\n			$height = $image[2];\n			$width = $image[1];\n		}\n\n		// Add all the logo attributes\n		$logo_attributes = apply_filters('Comunicherete_logo_image_attributes', array(\n			'src' => $src,\n			'width' => round($width),\n			'height' => round($height),\n			'alt' => sprintf( __('%s Logo', 'Comunicherete'), get_bloginfo('name') ),\n		) );\n\n		if($logo_attributes['width'] > Comunicherete_get_site_width()) {\n			// Don't let the width be more than the site width.\n			$width = Comunicherete_get_site_width();\n			$logo_attributes['height'] = round($logo_attributes['height'] / ($logo_attributes['width'] / $width));\n			$logo_attributes['width'] = $width;\n		}\n\n		$logo_attributes_str = array();\n		if( !empty( $logo_attributes ) ) {\n			foreach($logo_attributes as $name => $val) {\n				if( empty($val) ) continue;\n				$logo_attributes_str[] = $name.'=\"'.esc_attr($val).'\" ';\n			}\n		}\n		$logo_html = apply_filters('Comunicherete_logo_image', '<img '.implode($logo_attributes_str).' />');\n	}\n\n	// Echo the image\n	echo apply_filters('Comunicherete_logo_html', $logo_html);\n}\nendif;\n\n/**\n * Returns true if a blog has more than 1 category\n *\n * @since Comunicherete 1.0\n */\nfunction Comunicherete_categorized_blog() {\n	if ( false === ( $count = get_transient( 'Comunicherete_categorized_blog_cache_count' ) ) ) {\n		// Count the number of non-empty categories\n		$count = count( get_categories( array(\n			'hide_empty' => 1,\n		) ) );\n		\n		// Count the number of categories that are attached to the posts\n		set_transient( 'Comunicherete_categorized_blog_cache_count', $count );\n	}\n	\n	// Return true if this blog has categories, or else false.\n	return ($count >= 1);\n}\n\n/**\n * Flush out the transients used in Comunicherete_categorized_blog\n *\n * @since Comunicherete 1.0\n */\nfunction Comunicherete_category_transient_flusher() {\n	delete_transient( 'Comunicherete_categorized_blog_cache_count' );\n}\nadd_action( 'edit_category', 'Comunicherete_category_transient_flusher' );\nadd_action( 'save_post', 'Comunicherete_category_transient_flusher' );\n\n/**\n * Return the archive title depending on which page is being displayed.\n * \n * @since Comunicherete 1.0\n */\nfunction Comunicherete_get_archive_title(){\n	$title = '';\n	if ( is_category() ) {\n		$title = sprintf( __( 'Category Archives: %s', 'Comunicherete' ), '<span>' . single_cat_title( '', false ) . '</span>' );\n\n	}\n	elseif ( is_tag() ) {\n		$title = sprintf( __( 'Tag Archives: %s', 'Comunicherete' ), '<span>' . single_tag_title( '', false ) . '</span>' );\n\n	}\n	elseif ( is_author() ) {\n		the_post();\n		$title = sprintf( __( 'Author Archives: %s', 'Comunicherete' ), '<span class=\"vcard\"><a class=\"url fn n\" href=\"' . esc_url( get_author_posts_url( get_the_author_meta( \"ID\" ) ) ) . '\" title=\"' . esc_attr( get_the_author() ) . '\" rel=\"me\">' . get_the_author() . '</a></span>' );\n		rewind_posts();\n\n	}\n	elseif ( is_day() ) {\n		$title = sprintf( __( 'Daily Archives: %s', 'Comunicherete' ), '<span>' . get_the_date() . '</span>' );\n\n	}\n	elseif ( is_month() ) {\n		$title = sprintf( __( 'Monthly Archives: %s', 'Comunicherete' ), '<span>' . get_the_date( 'F Y' ) . '</span>' );\n\n	}\n	elseif ( is_year() ) {\n		$title = sprintf( __( 'Yearly Archives: %s', 'Comunicherete' ), '<span>' . get_the_date( 'Y' ) . '</span>' );\n\n	}\n	else {\n		$title = __( 'Archives', 'Comunicherete' );\n	}\n	\n	return apply_filters('Comunicherete_archive_title', $title);\n}\n\n/**\n * Get the post meta.\n * \n * @since Comunicherete 1.0\n */\nfunction Comunicherete_get_post_categories(){\n	/* translators: used between list items, there is a space after the comma */\n	$category_list = get_the_category_list( __( ', ', 'Comunicherete' ) );\n\n	/* translators: used between list items, there is a space after the comma */\n	$tag_list = get_the_tag_list( '', __( ', ', 'Comunicherete' ) );\n\n	if ( ! Comunicherete_categorized_blog() ) {\n		// This blog only has 1 category so we just need to worry about tags in the meta text\n		if ( '' != $tag_list ) {\n			$meta_text = __( '<strong>Tagged</strong> %2$s.', 'Comunicherete' );\n		} else {\n			$meta_text = '';\n		}\n\n	} else {\n		// But this blog has loads of categories so we should probably display them here\n		if ( '' != $tag_list ) {\n			$meta_text = __( 'Posted in %1$s and tagged %2$s.', 'Comunicherete' );\n		} else {\n			$meta_text = __( 'Posted in %1$s.', 'Comunicherete' );\n		}\n\n	} // end check for categories on this blog\n\n	$meta = sprintf(\n		$meta_text,\n		$category_list,\n		$tag_list,\n		get_permalink(),\n		the_title_attribute( 'echo=0' )\n	);\n	\n	return apply_filters('Comunicherete_post_meta', $meta);\n}\n\n/**\n * Gets the URL that should be displayed when clicking on an image in the view image page.\n * \n * @param null $post\n * @return string\n */\nfunction Comunicherete_next_attachment_url($post = null){\n	if(empty($post)){\n		global $post;\n	}\n	\n	/**\n	 * Grab the IDs of all the image attachments in a gallery so we can get the URL of the next adjacent image in a gallery,\n	 * or the first image (if we're looking at the last image in a gallery), or, in a gallery of one, just the link to that image file\n	 */\n	$attachments = array_values( get_children( array(\n		'post_parent'    => $post->post_parent,\n		'post_status'    => 'inherit',\n		'post_type'      => 'attachment',\n		'post_mime_type' => 'image',\n		'order'          => 'ASC',\n		'orderby'        => 'menu_order ID'\n	) ) );\n	foreach ( $attachments as $k => $attachment ) {\n		if ( $attachment->ID == $post->ID )\n			break;\n	}\n	$k++;\n	// If there is more than 1 attachment in a gallery\n	if ( count( $attachments ) > 1 ) {\n		if ( isset( $attachments[ $k ] ) ){\n			// get the URL of the next image attachment\n			$next_attachment_url = get_attachment_link( $attachments[ $k ]->ID );\n		}\n		else{\n			// or get the URL of the first image attachment\n			$next_attachment_url = get_attachment_link( $attachments[ 0 ]->ID );\n		}\n			\n	}\n	else {\n		// or, if there's only 1 image, get the URL of the image\n		$next_attachment_url = wp_get_attachment_url();\n	}\n	\n	return $next_attachment_url;\n}\n\n/**\n * Display the pagination\n *\n * @param string $pages\n * @param int $range\n */\nfunction Comunicherete_pagination($pages = '', $range = 2) {\n\n	$showitems = ($range * 2)+1;\n\n	global $wp_query, $wp_rewrite;\n	$paged = $wp_query->get('paged');\n	if(empty($paged)) $paged = 1;\n\n	if($pages == '') {\n		global $wp_query;\n		$pages = $wp_query->max_num_pages;\n		if(!$pages) $pages = 1;\n	}\n\n	if(1 != $pages) {\n		echo \"<div class='pagination'>\";\n		echo paginate_links(array(\n			'total' => $pages,\n			'current' => $paged,\n			'mid_size' => $showitems,\n			'format' => ( $wp_rewrite->permalink_structure == '' || is_search() ) ? ( strpos(get_pagenum_link(false), '?') === false ? '?paged=%#%' : '&paged=%#%' ) : 'page/%#%/',\n			'base' => get_pagenum_link(false).'%_%',\n		));\n		echo \"</div>\\n\";\n	}\n}",
