/label>\n			<input type=\"text\" class=\"widefat\" id=\"<?php echo $this->get_field_id('headline') ?>\" name=\"<?php echo $this->get_field_name('headline') ?>\" value=\"<?php echo esc_attr($instance['headline']) ?>\" />\n		</p>\n		<p>\n			<label for=\"<?php echo $this->get_field_id('sub_headline') ?>\"><?php _e('Sub Headline', 'Comunicherete') ?></label>\n			<input type=\"text\" class=\"widefat\" id=\"<?php echo $this->get_field_id('sub_headline') ?>\" name=\"<?php echo $this->get_field_name('sub_headline') ?>\" value=\"<?php echo esc_attr($instance['sub_headline']) ?>\" />\n		</p>\n		<?php\n	}\n}\n\n/**\n * A widget for display social media networks\n *\n * Class Comunicherete_Social_Media_Widget\n */\nclass Comunicherete_Social_Media_Widget extends WP_Widget{\n\n	private $networks;\n\n	/**\n	 * Register widget with WordPress.\n	 */\n	function __construct() {\n		parent::__construct(\n			'Comunicherete-social-media',\n			__('Comunicherete Social Media', 'Comunicherete'),\n			array(\n				'description' => __( 'Add nice little icons that link out to your social media profiles.', 'text_domain' )\n			)\n		);\n\n		$this->networks = apply_filters('Comunicherete_social_widget_networks', array(\n			'facebook' => __('Facebook', 'Comunicherete'),\n			'twitter' => __('Twitter', 'Comunicherete'),\n			'google-plus' => __('Google Plus', 'Comunicherete'),\n			'rss' => __('RSS', 'Comunicherete'),\n		));\n	}\n\n	public function widget( $args, $instance ) {\n		// outputs the content of the widget\n		echo $args['before_widget'];\n\n		if(!empty($instance['title'])) {\n			echo $args['before_title'].$instance['title'].$args['after_title'];\n		}\n\n		foreach($this->networks as $id => $name) {\n			if(!empty($instance[$id])) {\n				?><a class=\"social-media-icon social-media-icon-<?php echo $id ?> social-media-icon-<?php echo esc_attr($instance['size']) ?>\" href=\"<?php echo esc_url( $instance[$id] ) ?>\" title=\"<?php echo esc_html( get_bloginfo('name') . ' ' . $name ) ?>\" <?php if(!empty($instance['new_window'])) echo 'target=\"_blank\"'; ?>><?php\n\n				$icon = apply_filters('Comunicherete_social_widget_icon_'.$id, '');\n				if(!empty($icon)) echo $icon;\n				else echo '<span class=\"icon-' . $id . '\"></span>';\n\n				?></a><?php\n			}\n		}\n\n		echo $args['after_widget'];\n	}\n\n	public function form( $instance ) {\n		$instance = wp_parse_args($instance, array(\n			'size' => 'medium',\n			'title' => '',\n			'new_window' => false,\n		) );\n\n		$sizes = apply_filters('Comunicherete_social_widget_sizes', array(\n			'medium' => __('Medium', 'Comunicherete'),\n		));\n\n		?>\n		<p>\n			<label for=\"<?php echo $this->get_field_id('title') ?>\"><?php _e('Title') ?></label><br/>\n			<input type=\"text\" name=\"<?php echo $this->get_field_name('title') ?>\" id=\"<?php echo $this->get_field_id('title') ?>\" value=\"<?php echo esc_attr($instance['title']) ?>\" />\n		</p>\n		<p>\n			<label for=\"<?php echo $this->get_field_id('size') ?>\"><?php _e('Icon Size') ?></label><br/>\n			<select id=\"<?php echo $this->get_field_id('size') ?>\" name=\"<?php echo $this->get_field_name('size') ?>\">\n				<?php foreach($sizes as $id => $name) : ?>\n					<option value=\"<?php echo esc_attr($id) ?>\" <?php selected($instance['size'], $id) ?>><?php echo esc_html($name) ?></option>\n				<?php endforeach; ?>\n			</select>\n		</p>\n		<?php\n\n		foreach($this->networks as $id => $name) {\n			?>\n			<p>\n				<label for=\"<?php echo $this->get_field_id($id) ?>\"><?php echo $name ?></label>\n				<input type=\"text\" id=\"<?php echo $this->get_field_id($id) ?>\" name=\"<?php echo $this->get_field_name($id) ?>\" value=\"<?php echo esc_attr(!empty($instance[$id]) ? $instance[$id] : '') ?>\" class=\"widefat\"/>\n			</p>\n		<?php\n		}\n\n		?>\n		<p>\n			<input type=\"checkbox\" name=\"<?php echo $this->get_field_name('new_window') ?>\" id=\"<?php echo $this->get_field_id('new_window') ?>\" <?php checked($instance['new_window']) ?> />\n			<label for=\"<?php echo $this->get_field_id('new_window') ?>\"><?php _e('Open in New Window') ?></label>\n\n		</p>\n		<?php\n\n		if(!defined('SITEORIGIN_IS_PREMIUM')) {\n			?>\n			<p style=\"background: #cbe385; padding: 8px;\">\n				<?php printf(__('Get additional social and professional network icons and sizes in <a href=\"%s\" target=\"_blank\">Comunicherete Premium</a>.'), admin_url('themes.php?page=premium_upgrade')) ?>\n			</p>\n			<?php\n		}\n	}\n\n	public function update( $new_instance, $old_instance ) {\n		$new_instance['new_window'] = !empty($new_instance['new_window']);\n		return $new_instance;\n	}\n}\n\n/**\n * Register the Comunicherete specific widgets.\n */\nfunction Comunicherete_register_widgets(){\n	register_widget('Comunicherete_Social_Media_Widget');\n	register_widget('Comunicherete_CircleIcon_Widget');\n	register_widget('Comunicherete_Headline_Widget');\n}\nadd_action( 'widgets_init', 'Comunicherete_register_widgets');\n\n/**\n * Filter the carousel loop title to add navigation controls.\n */\nfunction Comunicherete_filter_carousel_loop($title, $instance = array(), $id = false){\n	if($id == 'siteorigin-panels-postloop' && isset($instance['template']) && $instance['template'] == 'loops/loop-carousel.php') {\n		$title = '<span class=\"Comunicherete-carousel-title\"><span class=\"Comunicherete-carousel-title-text\">'.$title.'</span><a href=\"#\" class=\"next\">next</a><a href=\"#\" class=\"previous\">previous</a></span>';\n	}\n	return $title;\n}\nadd_filter('widget_title', 'Comunicherete_filter_carousel_loop', 10, 3);\n\n/**\n * Handle ajax requests for the carousel.\n */\nfunction Comunicherete_carousel_ajax_handler(){\n	if(empty($_GET['query'])) return;\n\n	$query = $_GET['query'];\n	$query['paged'] = $_GET['paged'];\n\n	query_posts($query);\n	ob_start();\n	get_template_part('loops/loop', 'carousel');\n\n	global $wp_query;\n	$count = $wp_query->post_count;\n\n	// Reset everything\n	wp_reset_query();\n	wp_reset_postdata();\n\n	header('content-type:application/json');\n	echo json_encode( array(\n		'html' => ob_get_clean(),\n		'count' => $count,\n	) );\n\n	exit();\n}\nadd_action('wp_ajax_Comunicherete_carousel_load', 'Comunicherete_carousel_ajax_handler');\nadd_action('wp_ajax_nopriv_Comunicherete_carousel_load', 'Comunicherete_carousel_ajax_handler');",
