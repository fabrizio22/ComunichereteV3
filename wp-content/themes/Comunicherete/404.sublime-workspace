ary:\n	if ( $paged >= 2 || $page >= 2 )\n		$title .= \" $sep \" . sprintf( __( 'Page %s', 'Comunicherete' ), max( $paged, $page ) );\n\n	return $title;\n}\nadd_filter( 'wp_title', 'Comunicherete_wp_title', 10, 2 );\n\n/**\n * Add the styles to set the size of the footer widgets\n * \n * @todo generate responsive code\n */\nfunction Comunicherete_footer_widget_style(){\n	$widgets = wp_get_sidebars_widgets();\n	if(empty($widgets['sidebar-footer'])) return;\n\n	$count = count($widgets['sidebar-footer']);\n	?><style type=\"text/css\" id=\"Comunicherete-footer-widgets\">#footer-widgets aside { width : <?php echo round(100/$count,3) ?>%; } </style> <?php\n}\nadd_action('wp_head', 'Comunicherete_footer_widget_style', 15);",
